#%Module################################################
# Load in existing modules
module load netcdf
module load udunits/2.2.26 


# Set new environment variables here
set version 2.1.7
set app     ncview
set NCVIEWROOT /home/home02/earhbu/
module-whatis "netCDF view - netCDF file viewer "
prepend-path PATH $NCVIEWROOT/bin

proc ModulesHelp { } {
    puts stderr "netCDF view - netCDF file viewer

Information:
  Homepage : http://meteora.ucsd.edu/~pierce/ncview_home_page.html

License:
  Summary  : GNU GPL3
 "
}


